    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-email/components": "^0.3.2",
    "@react-email/render": "^1.1.3",
    "@shopify/hydrogen-react": "^2025.5.0",
    "@shopify/shopify-api": "^11.13.0",
    "@shopify/storefront-api-client": "^1.0.8",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.52.0",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@trpc/client": "^11.4.3",
    "@trpc/server": "^11.4.3",
    "bullmq": "^5.56.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "graphql-request": "^7.2.0",
    "ioredis": "^5.6.1",
    "lucide-react": "^0.525.0",
    "next": "15.4.2",
    "next-themes": "^0.4.6",
    "papaparse": "^5.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.1.0",
    "resend": "^4.7.0",
    "superjson": "^2.2.2",
    "swr": "^2.3.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.0.5",
    "@langchain/openai": "^0.2.5",
    "js-tiktoken": "^1.0.14",
    "langchain": "^0.2.19",
    "openai": "^4.67.3",
    "@tensorflow/tfjs": "^4.26.0",
    "@tensorflow/tfjs-node": "^4.26.0",
    "framer-motion": "^11.14.1",
    "next-auth": "^4.24.10"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/crypto-js": "^4.2.2",
    "@types/papaparse": "^5.3.14",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.0",
    "eslint": "^9.31.0",
    "eslint-config-next": "^15.4.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.12.1",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-import-resolver-typescript": "^3.7.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.3.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "@svgr/webpack": "^8.1.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "tailwindcss": "^4.1.11",
