{
  "name": "mercury",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev --turbopack",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:standalone": "BUILD_STANDALONE=true next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint --max-warnings 0",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "worker": "tsx lib/sync/worker.ts",
    "worker:dev": "tsx watch lib/sync/worker.ts",
    "worker:prod": "NODE_ENV=production tsx lib/sync/worker.ts",
    "db:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > lib/database.types.ts",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "postinstall": "npm run type-check",
    "clean": "rm -rf .next out dist build coverage",
    "clean:all": "npm run clean && rm -rf node_modules",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "health": "curl -f http://localhost:3000/api/health || exit 1",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
